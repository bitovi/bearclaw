{
  "name": "bearclaw",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "remix build",
    "mocks": "run-p mocks:*",
    "dev": "run-p dev:*",
    "mocks:remix": "cross-env NODE_ENV=development binode --require ./app/msw/msw.server.js -- @remix-run/dev:remix dev",
    "dev:remix:": "cross-env NODE_ENV=development binode -- @remix-run/dev:remix dev",
    "database": "docker-compose -f docker-compose-db.yml up --build",
    "docker": "docker-compose -f docker-compose-dev.yml up --build",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "db:reset": "prisma migrate reset && prisma db seed",
    "start": "remix-serve build",
    "start:mocks": "binode --require ./app/msw/msw.server.js -- @remix-run/serve:remix-serve build",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
    "typecheck": "tsc && tsc -p cypress",
    "validate:quick": "run-p \"test -- --run\" lint typecheck",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:jekyll": "touch storybook-static/.nojekyll",
    "storybook:deploy": "gh-pages --dotfiles --dist storybook-static",
    "prepare": "husky install",
    "postinstall": "patch-package"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "storybook-static",
    "/docs",
    "/sanity"
  ],
  "dependencies": {
    "@cyclonedx/cyclonedx-library": "2.0.0",
    "@emotion/cache": "^11.10.8",
    "@emotion/react": "^11.10.8",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.8",
    "@faker-js/faker": "^8.0.2",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.2",
    "@paralleldrive/cuid2": "^2.2.0",
    "@portabletext/react": "^3.0.2",
    "@prisma/client": "^4.14.1",
    "@remix-run/node": "^1.16.1",
    "@remix-run/react": "^1.16.1",
    "@remix-run/serve": "^1.16.1",
    "@remix-run/server-runtime": "^1.16.1",
    "@sentry/remix": "^7.47.0",
    "@stripe/react-stripe-js": "^2.1.0",
    "@stripe/stripe-js": "^1.52.1",
    "bcryptjs": "^2.4.3",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "gravatar": "^1.8.2",
    "isbot": "^3.6.7",
    "lodash": "^4.17.21",
    "msw": "^1.2.1",
    "patch-package": "^7.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stripe": "^12.1.1",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@portabletext/types": "^2.0.2",
    "@remix-run/dev": "^1.16.1",
    "@remix-run/eslint-config": "^1.16.1",
    "@storybook/addon-essentials": "^7.0.2",
    "@storybook/addon-interactions": "^7.0.2",
    "@storybook/addon-links": "^7.0.2",
    "@storybook/blocks": "^7.0.2",
    "@storybook/builder-vite": "^7.0.7",
    "@storybook/react": "^7.0.2",
    "@storybook/react-vite": "^7.0.7",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@testing-library/cypress": "^8.0.7",
    "@testing-library/dom": "^8.20.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.21.2",
    "@types/gravatar": "^1.8.3",
    "@types/node": "^18.15.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/spdx-expression-parse": "^3.0.2",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.3",
    "autoprefixer": "^10.4.14",
    "binode": "^1.0.5",
    "c8": "^7.13.0",
    "cookie": "^0.5.0",
    "cross-env": "^7.0.3",
    "cypress": "^10.11.0",
    "cypress-real-events": "^1.8.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-testing-library": "^5.10.3",
    "gh-pages": "^5.0.0",
    "happy-dom": "^8.9.0",
    "husky": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "2.8.4",
    "prisma": "^4.14.1",
    "prisma-dbml-generator": "^0.10.0",
    "start-server-and-test": "^1.15.4",
    "storybook": "^7.0.2",
    "storybook-addon-react-router-v6": "^1.0.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-tsconfig-paths": "^3.6.0",
    "vitest": "^0.29.3"
  },
  "engines": {
    "node": ">=18"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}
